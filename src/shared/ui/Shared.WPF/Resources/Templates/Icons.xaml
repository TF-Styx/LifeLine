        <Viewbox Stretch="Uniform">
            <Canvas Width="24" Height="24">
                <Path Stroke="{Binding Foreground, RelativeSource={RelativeSource TemplatedParent}}"
                  StrokeThickness="1"
                  StrokeLineJoin="Miter"
                  Data="
                  M 8 22 L 2 22 L 2 16
                  M 22 16 L 22 22 L 16 22
                  M 16 2 L 22 2 L 22 8
                  M 2 8 L 2 2 L 8 2
                  "/>
            </Canvas>
        </Viewbox>
    </ControlTemplate>

    <!-- Иконка "Лупа" -->
    <ControlTemplate x:Key="Magnifier" TargetType="Control">
        <Viewbox Stretch="Uniform">
            <!-- Размеры взяты из viewBox SVG: "0 0 36 36" -->
            <Grid Width="36" Height="36">
                <!-- 
                Порядок элементов сохранен из SVG для правильного наложения.
                -->

                <!-- 1. Соединительная часть между ручкой и линзой -->
                <Path Fill="#9AAAB4" 
                      Data="M13.503 19.693l2.828 2.828l-4.95 4.95l-2.828-2.829z" />

                <!-- 2. Ручка лупы -->
                <Path Fill="#66757F" 
                      Data="M1.257 29.11l5.88-5.879a2 2 0 0 1 2.828 0l2.828 2.828a2 2 0 0 1 0 2.828l-5.879 5.879a4 4 0 1 1-5.657-5.656z" />

                <!-- 3. Внешний обод лупы (преобразовано из <circle>) -->
                <!-- cx=22.355, cy=13.669, r=13.5 -->
                <!-- Width/Height = 2*r = 27 -->
                <!-- Margin Left = cx-r = 8.855 -->
                <!-- Margin Top = cy-r = 0.169 -->
                <Ellipse Fill="#8899A6" Width="27" Height="27" 
                         HorizontalAlignment="Left" VerticalAlignment="Top" 
                         Margin="8.855,0.169,0,0" />

                <!-- 4. Внутренняя линза (преобразовано из <circle>) -->
                <!-- cx=22.355, cy=13.669, r=9.5 -->
                <!-- Width/Height = 2*r = 19 -->
                <!-- Margin Left = cx-r = 12.855 -->
                <!-- Margin Top = cy-r = 4.169 -->
                <Ellipse Fill="#BBDDF5" Width="19" Height="19" 
                         HorizontalAlignment="Left" VerticalAlignment="Top" 
                         Margin="12.855,4.169,0,0" />
            </Grid>
        </Viewbox>
    </ControlTemplate>

    <!-- Иконка "Добавить" -->
    <ControlTemplate x:Key="AddIcon" TargetType="Control">
        <Viewbox Stretch="Uniform">
            <!-- Размеры взяты из viewBox SVG: "0 0 24 24" -->
            <Grid Width="24" Height="24">

                <!-- 1. Контур круга (преобразовано из <circle>) -->
                <!-- cx=12, cy=12, r=10 -->
                <!-- Width/Height = 2*r = 20 -->
                <!-- Margin = (24 - 20) / 2 = 2 -->
                <Ellipse Stroke="White" StrokeThickness="1.5" 
                         Width="20" Height="20" 
                         HorizontalAlignment="Center" VerticalAlignment="Center" />

                <!-- 2. Знак "плюс" -->
                <!-- Я объединил 4 линии в 2 для оптимизации -->
                <Path Stroke="White" StrokeThickness="1.5" 
                      StrokeStartLineCap="Round" StrokeEndLineCap="Round"
                      Data="M 9,12 H 15 M 12,9 V 15" />
            </Grid>
        </Viewbox>
    </ControlTemplate>

